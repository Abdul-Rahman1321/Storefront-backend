{
  "types": "module",
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "jasmine": "jasmine",
    "tsc": "tsc",
    "prestart": "npm run build",
    "db:up": "db-migrate up",
    "db:down": "db-migrate reset",
    "dev": "nodemon src/server.ts --watch",
    "lint": "eslint --ext .ts src",
    "build": "npx tsc",
    "prettier": "npx prettier --write ./src/**/*.ts",
    "ex": "npm run build&&set ENV=test&&db-migrate --env test up",
    "test": "npm run build&&set ENV=test&&db-migrate --env test up&&jasmine&&db-migrate --env test reset"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.15",
    "pg": "^8.5.1",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.10.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "jasmine": "^4.0.2",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.4.0",
    "morgan": "^1.10.0",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.5.5"
  }
}
